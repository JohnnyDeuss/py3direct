{
	"manifest_version": 2,

	"name": "py3direct",
	"description": "Redirect the Python 2 docs to Python 3 docs",
	"version": "1.0",

	"background": {
		"scripts": ["uri.js", "events.js"]
	},
	"options_ui": {
		"page": "options.html"
	},
	"icons": {"128": "assets/icon128.png"},
	"browser_action": {
	"default_icon": "assets/icon38.png"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"https://docs.python.org/*"
	]
}
